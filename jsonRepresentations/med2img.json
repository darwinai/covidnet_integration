      "action": "store_true",
      "help": "save images along i, j, k direction -- 3D input only",
      "default": false,
      "ui_exposed": true
    },
    {
      "name": "showSlices",
      "type": "bool",
      "optional": true,
      "flag": "--showSlices",
      "short_flag": "--showSlices",
