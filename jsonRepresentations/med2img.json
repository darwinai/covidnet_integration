      "short_flag": "-i",
      "action": "store",
      "help": "name of the input file within the inputDir",
      "default": null,
      "ui_exposed": true
    },
    {
      "name": "outputFileStem",
      "type": "str",
      "optional": true,
      "flag": "--outputFileStem",
      "short_flag": "-o",
      "action": "store",
      "help": "output file",
      "default": "sample",
      "ui_exposed": true
    },
    {
      "name": "outputFileType",
      "type": "str",
      "optional": true,
      "flag": "--outputFileType",
      "short_flag": "-t",
      "action": "store",
      "help": "output image file format",
      "default": "jpg",
      "ui_exposed": true
    },
    {
      "name": "sliceToConvert",
      "type": "str",
      "optional": true,
      "flag": "-s--sliceToConvert",
      "short_flag": "-s--sliceToConvert",
      "action": "store",
      "help": "slice to convert (for 3D data)",
      "default": "-1",
      "ui_exposed": true
    },
    {
      "name": "frameToConvert",
      "type": "str",
      "optional": true,
      "flag": "--frameToConvert",
      "short_flag": "-f",
